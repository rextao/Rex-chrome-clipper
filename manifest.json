{
  "name": "html保存到本地",
  "manifest_version":2,
  "version": "1.0",
  "description": "为html添加注释后保存到本地",
  "browser_action": {
    "default_icon": "icon.png" ,
    "default_title": "rextao",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["lib/jquery.3.3.1.min.js","js/main.min.js"],
      "css": ["css/main.min.css"],
      "run_at": "document_start"
    }
  ]
}